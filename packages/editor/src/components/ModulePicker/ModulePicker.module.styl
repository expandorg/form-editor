@import '~@expandorg/uikit/colors';
@import '~@expandorg/uikit/font';

.container
  box-sizing: border-box;

  height: 100%
  width: 100%
  position: relative;

.list
  box-sizing: content-box;

  height: calc(100% - 55px)
  width: 100%

  overflow-y: auto;

.empty
  font(input-label, 800, 12px)
  color: color-white
  text-align: center
  padding-top: 30px
